    "from manim import *",
    "",
    "class CyclicReplaceExample(Scene):",
    "    def construct(self):",
    "        group = VGroup(Square(), Circle(), Triangle(), Star())",
    "        group.arrange(RIGHT)",
    "        self.add(group)",
    "",
    "        for _ in range(4):",
    "            self.play(CyclicReplace(*group))"
