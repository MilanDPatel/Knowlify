    "from manim import *",
    "",
    "class ShellLayout(Scene):",
    "    def construct(self):",
    "        nlist = [[1, 2, 3], [4, 5, 6, 7, 8, 9]]",
    "        graph = Graph(",
    "            [1, 2, 3, 4, 5, 6, 7, 8, 9],",
