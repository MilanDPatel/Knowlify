    "from manim import *",
    "",
    "class GetYAxisLabelExample(Scene):",
    "    def construct(self):",
    "        ax = Axes(x_range=(0, 8), y_range=(0, 5), x_length=8, y_length=5)",
    "        y_label = ax.get_y_axis_label(",
    "            Tex(\"$y$-values\").scale(0.65).rotate(90 * DEGREES),",
    "            edge=LEFT,",
    "            direction=LEFT,",
    "            buff=0.3,",
    "        )",
    "        self.add(ax, y_label)"
