    "from manim import *",
    "",
    "class GetGraphLabelExample(Scene):",
    "    def construct(self):",
    "        ax = Axes()",
    "        sin = ax.plot(lambda x: np.sin(x), color=PURPLE_B)",
    "        label = ax.get_graph_label(",
    "            graph=sin,",
    "            label= MathTex(r\"\\frac{\\pi}{2}\"),",
    "            x_val=PI / 2,",
    "            dot=True,",
    "            direction=UR,",
    "        )",
    "",
    "        self.add(ax, sin, label)"
