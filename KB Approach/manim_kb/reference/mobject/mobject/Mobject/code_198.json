    "from manim import *",
    "",
    "class AnimateExample(Scene):",
    "    def construct(self):",
    "        s = Square()",
    "        self.play(Create(s))",
    "        self.play(s.animate.shift(RIGHT))",
    "        self.play(s.animate.scale(2))",
    "        self.play(s.animate.rotate(PI / 2))",
    "        self.play(Uncreate(s))"
